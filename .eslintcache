1>\n    }\n}\n\n\nexport default EventPage","/Users/ekene/Development/code/Mod4/ovation-project/ovation_frontend/src/components/pages/LoginPage.js",[],"/Users/ekene/Development/code/Mod4/ovation-project/ovation_frontend/src/components/pages/UserPage.js",[],"/Users/ekene/Development/code/Mod4/ovation-project/ovation_frontend/src/components/Search.js",[],"/Users/ekene/Development/code/Mod4/ovation-project/ovation_frontend/src/components/EventItem.js",["63","64","65","66"],"import { Link } from \"react-router-dom\";\nimport {useHistory} from \"react-router-dom\"\n\nfunction EventItem ( {event}) {\n    const history = useHistory()\n    \n    const { id, name, artist, description, time, date } = event \n\n    function changeOpacity(evt) {\n        console.log(evt.target)\n    }\n    \n    return (\n        <div className=\"event-card\">\n            <div className=\"all-event-image-div\"> \n                <img src={event.artist.picture} alt={event.artist.name} onMouseOver={changeOpacity} onClick={() => history.push(`/events/${event.id}`)}></img>\n            </div>\n            <div className=\"event-text-content\">\n                <h1>{name}</h1> \n                <p>Who -- {artist.name}</p>\n                <p>Date -- {date} </p>\n                <p>Time -- {event.time ? event.time.slice(11, 16) : \"TBA\"}</p>\n            </div>\n        </div>\n    )\n}\n\nexport default EventItem",{"ruleId":"67","replacedBy":"68"},{"ruleId":"69","replacedBy":"70"},{"ruleId":"71","severity":1,"message":"72","line":2,"column":10,"nodeType":"73","messageId":"74","endLine":2,"endColumn":14},{"ruleId":"71","severity":1,"message":"75","line":10,"column":11,"nodeType":"73","messageId":"74","endLine":10,"endColumn":18},{"ruleId":"71","severity":1,"message":"76","line":43,"column":22,"nodeType":"73","messageId":"74","endLine":43,"endColumn":26},{"ruleId":"71","severity":1,"message":"77","line":43,"column":36,"nodeType":"73","messageId":"74","endLine":43,"endColumn":40},{"ruleId":"71","seve